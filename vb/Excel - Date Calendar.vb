    INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 > 260,
    INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 - 260,
    IF(
        INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 > 208,
        INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 - 208,
        IF(
            INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 > 156,
            INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 - 156,
            IF(
                INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 > 104,
                INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 - 104,
                IF(
                    INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 > 52,
                    INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1 - 52,
                    INT(([@Date] - DATE(2022, 01, 01)) / 7) + 1
